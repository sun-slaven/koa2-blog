{
    "extends": ["airbnb-base"],
    "plugins": [
       "import",
       "html"
    ],
    "env": {
        "browser": true,
        "node": true,
        "es6": true
    },
     "rules":{
        "no-console": "off",
        "no-param-reassign": "off",
        "eol-last": "off",
        "comma-dangle": "off",
        "newline-after-var": "off",
        "import/newline-after-import": "off",
        "no-unused-vars": "warn",
        "indent": ["warn", 4],
        "class-methods-use-this": "off",
        "no-empty-function": "off",
        "no-useless-constructor": "off",
        "no-new": "off"
    }
}